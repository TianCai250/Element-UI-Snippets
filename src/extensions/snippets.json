    "el-select": {
        "prefix": "select",
        "body": [
            "<el-select v-model=\"value\" placeholder=\"请选择\">",
            "  <el-option",
            "    v-for=\"item in options\"",
            "    :key=\"item.value\"",
            "    :label=\"item.label\"",
            "    :value=\"item.value\">",
            "  </el-option>",
            "</el-select>"
        ],
        "description": "选择器"
    },
    "el-cascader": {
        "prefix": "cascader",
        "body": [
            "<el-cascader",
            "    v-model=\"value\"",
            "    :options=\"options\"",
            "    @change=\"handleChange\"></el-cascader>"
        ],
        "description": "级联选择器"
    },
    "el-switch": {
        "prefix": "switch",
        "body": [
            "<el-switch",
            "  v-model=\"value\"",
            "  active-color=\"#13ce66\"",
            "  inactive-color=\"#ff4949\">",
            "</el-switch>"
        ],
        "description": "开关"
    },
    "el-slider": {
        "prefix": "slider",
        "body": [
            "<el-slider v-model=\"value\"></el-slider>"
        ],
        "description": "滑块"
    },
    "el-time-picker": {
        "prefix": "time-picker",
        "body": [
            "<el-time-picker",
            "  v-model=\"value\"",
            "  :picker-options=\"{",
            "    selectableRange: '18:30:00 - 20:30:00'",
            "  }\"",
            "  placeholder=\"任意时间点\">",
            "</el-time-picker>",
            "<el-time-picker",
            "  arrow-control",
            "  v-model=\"value\"",
            "  :picker-options=\"{",
            "    selectableRange: '18:30:00 - 20:30:00'",
            "  }\"",
            "  placeholder=\"任意时间点\">",
            "</el-time-picker>"
        ],
        "description": "时间选择器"
    },
    "el-date-picker": {
        "prefix": "date-picker",
        "body": [
            "<el-date-picker",
            "  v-model=\"value\"",
            "  type=\"date\"",
            "  placeholder=\"选择日期\">",
            "</el-date-picker>"
        ],
