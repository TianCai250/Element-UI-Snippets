{
    "name": "vue-element-ui-snippets",
    "displayName": "Element-UI Snippets",
    "description": "ElementUI的代码提示插件",
    "keywords": [
        "element",
        "ui",
        "snippets",
        "饿了吗",
        "代码片段"
    ],
    "version": "0.0.5",
    "publisher": "wangluo",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Snippets"
    ],
    "icon": "images/logo.png",
    "activationEvents": [
        "onLanguage:vuejs"
    ],
    "main": "./src/extension",
    "contributes": {
        "snippets": [
            {
                "language": "vue",
                "path": "./src/extensions/snippets.json"
            },
            {
                "language": "html",
                "path": "./src/extensions/snippets.json"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "build": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.67.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "14.x",
        "eslint": "^8.14.0",
        "glob": "^8.0.1",
        "mocha": "^9.2.2",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    },
    "license": "SEE LICENSE IN LICENSE.txt"
}